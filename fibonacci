def fibo(num):
    a=1
    b=1
    print(a)
    print(b)
    for i in range(2,20):
        c=a+b
        a=b
        b=c
        print(c)

fibo(5)

#copy a image and save it into another jpg
f=open('pic.PNG','rb')       #read binary means pic is stored in binary and we can only read pic by rb

f1=open('mypic.JPG','wb')
for i in f:
   f1.write(i)

