def sort(num):          #bubble sort
   for i in range(len(num)-1,0,-1):
       for j in range(i):
           if num[j]>num[j+1]:
               num[j],num[j+1]=num[j+1],num[j]



def sorts(num):     #selection sort
    for i in range(len(num)-1):
        minpos=i
        for j in range(i,len(num)-1):
            if num[j]<num[minpos]:
                minpos=j
        num[minpos],num[i]=num[i],num[minpos]
        print(num)

num=[7,6,4,2,5,9]
sorts(num)
print(num)

